{
    "family": "fargate-lab9-docker",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "fargate-app-lab9",
            "image": "evr0518/fm-java:docker-java-1.0",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
